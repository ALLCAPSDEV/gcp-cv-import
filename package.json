{
  "name": "@allcapsdev/gcp-cv-import",
  "version": "0.0.50",
  "description": "",
  "main": "./lib/index.js",
  "private": false,
  "bin": {
    "cvcsvimport": "./bin/index.js"
  },
  "files": [
    "lib/**/*",
    "NOTICE"
  ],
  "scripts": {
    "build": "npm run clean:some && rollup -c",
    "clean:lib": "rimraf ./lib",
    "clean:docs": "rimraf ./docs",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:some": "run-p clean:lib clean:docs",
    "clean": "run-p prettier clean:lib clean:docs clean:node_modules",
    "commit": "git-cz",
    "commitlint": "commitlint -e",
    "release": "standard-version",
    "start:dev": "ts-node -T src/index.ts",
    "tscov": "tscov",
    "tscov:d": "tscov --details",
    "test": "jest",
    "prepare": "npm run build",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "pretty-quick": "pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ALLCAPSDEV/gcp-cv-import.git"
  },
  "keywords": [
    "Google Cloud Vision Product Search"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ALLCAPSDEV/gcp-cv-import/issues"
  },
  "homepage": "https://github.com/ALLCAPSDEV/gcp-cv-import#readme",
  "devDependencies": {
    "@arkweid/lefthook": "0.7.2",
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@liftr/tscov": "1.4.7",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "25.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.21.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-terser": "6.1.0",
    "rollup-plugin-typescript2": "0.27.1",
    "standard-version": "8.0.0",
    "ts-jest": "25.5.1",
    "ts-loader": "8.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  },
  "dependencies": {
    "@google-cloud/vision": "2.1.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
