{
  "name": "@allcapsdev/gcp-cv-import",
  "version": "0.0.18",
  "description": "",
  "main": "./lib/index.js",
  "private": false,
  "bin": {
    "cvcsvimport": "./bin/index.js"
  },
  "files": [
    "lib/**/*",
    "NOTICE"
  ],
  "scripts": {
    "build": "npm run clean:some && webpack",
    "postbuild": "npm run typedoc",
    "clean:lib": "rimraf ./lib",
    "clean:docs": "rimraf ./docs",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:some": "run-p clean:lib clean:docs",
    "clean": "run-p prettier clean:lib clean:docs clean:node_modules",
    "commit": "git-cz",
    "commitlint": "commitlint -e",
    "release": "standard-version -s",
    "start:dev": "ts-node -T src/index.ts",
    "tscov": "tscov",
    "tscov:d": "tscov --details",
    "test": "jest",
    "posttest": "npm run size",
    "typedoc": "typedoc --out docs ./src",
    "prepare": "npm run build",
    "prettier": "prettier --parser typescript --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "pretty-quick": "pretty-quick",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ALLCAPSDEV/gcp-cv-import.git"
  },
  "keywords": [
    "Google Cloud Vision Product Search"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ALLCAPSDEV/gcp-cv-import/issues"
  },
  "homepage": "https://github.com/ALLCAPSDEV/gcp-cv-import#readme",
  "devDependencies": {
    "@arkweid/lefthook": "0.7.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@liftr/tscov": "1.4.6",
    "@size-limit/preset-small-lib": "4.4.5",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.4",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "commitizen": "4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "25.5.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.2",
    "size-limit": "4.4.5",
    "standard-version": "7.1.0",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.1",
    "ts-node": "8.9.1",
    "typedoc": "0.17.6",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@google-cloud/vision": "1.11.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "size-limit": [
    {
      "limit": "2 KB",
      "config": "./webpack.config.js",
      "gzip": false
    }
  ]
}
